{
  "kind": "collectionType",
  "collectionName": "sequences",
  "info": {
    "singularName": "sequence",
    "pluralName": "sequences",
    "displayName": "Sequence",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "versions": {
      "versioned": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext"
    },
    "stories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::story.story",
      "mappedBy": "sequence"
    },
    "collections": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::collection.collection",
      "inversedBy": "sequences"
    },
    "promoImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "promoImageMobile": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "moreToCome": {
      "type": "boolean",
      "default": true,
      "required": true
    }
  }
}
