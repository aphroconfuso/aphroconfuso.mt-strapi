{
  "kind": "collectionType",
  "collectionName": "appointments",
  "info": {
    "singularName": "appointment",
    "pluralName": "appointments",
    "displayName": "Appointments",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "versions": {
      "versioned": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "editorial": {
      "type": "customField",
      "options": {
        "output": "HTML",
        "preset": "light"
      },
      "customField": "plugin::ckeditor.CKEditor",
      "required": true
    },
    "dateTimePublication": {
      "type": "datetime",
      "required": true
    },
    "stories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::story.story"
    },
    "moreToCome": {
      "type": "customField",
      "options": {
        "output": "HTML",
        "preset": "light"
      },
      "customField": "plugin::ckeditor.CKEditor"
    }
  }
}
