{
  "kind": "singleType",
  "collectionName": "homepages",
  "info": {
    "singularName": "homepage",
    "pluralName": "homepages",
    "displayName": "Homepage",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "versions": {
      "versioned": true
    }
  },
  "attributes": {
    "appointment": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::appointment.appointment"
    },
    "promos": {
      "type": "component",
      "repeatable": true,
      "component": "promotions.story-promo",
      "required": true,
      "max": 24,
      "min": 6
    },
    "imagePromos": {
      "type": "component",
      "repeatable": true,
      "component": "promotions.story-image-promo",
      "required": true,
      "max": 24,
      "min": 0
    },
    "poetryPromos": {
      "type": "component",
      "repeatable": true,
      "component": "promotions.story-poem-promo",
      "required": true,
      "max": 24,
      "min": 0
    },
    "showNewsletter": {
      "type": "boolean",
      "default": true
    },
    "layout": {
      "type": "enumeration",
      "enum": [
        "Layout 1",
        "Layout 2",
        "Layout 3",
        "Layout 4",
        "Layout 5",
        "Layout 6",
        "Layout 7",
        "Layout 8",
        "Layout 9",
        "Layout 10",
        "Layout 11",
        "Layout 12"
      ],
      "default": "Layout 1",
      "required": true
    },
    "showPodcastPromo": {
      "pluginOptions": {
        "versions": {
          "versioned": true
        }
      },
      "type": "boolean",
      "default": true,
      "required": false
    },
    "audioPromos": {
      "type": "component",
      "repeatable": true,
      "component": "promotions.audio"
    }
  }
}
